<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.waiter.mapper.MessageMapper">
    <select id="selectAllMessage" resultType="com.waiter.entity.Message">
        select *
        from message
        where receive_user = #{receive_user};
    </select>
    <select id="queryMessage" resultType="com.waiter.entity.Message">
        select *
        from message
        where message_id = #{message_id}
    </select>
</mapper>